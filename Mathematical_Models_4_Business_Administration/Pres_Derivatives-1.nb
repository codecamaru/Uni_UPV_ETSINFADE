       RowBox[{"(", 
        SubscriptBox["t", "0"], ")"}], " ", 
       RowBox[{"g", "'"}], 
       RowBox[{"(", 
        RowBox[{"f", "(", 
         SubscriptBox["t", "0"], ")"}], ")"}]}], "=", 
      RowBox[{"2", 
       SubsuperscriptBox["t", "0", "2"], " ", "cos", " ", 
       SubsuperscriptBox["t", "0", "3"]}]}]}]}], TraditionalForm]],
  ExpressionUUID->"590c7940-c231-491a-9af4-de2be18c3a74"],
