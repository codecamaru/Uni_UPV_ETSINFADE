Cell[CellGroupData[{

Cell["\<\
How to find the local maxima and minima of a function: 2nd criterion\
\>", "Subsection",
 CellChangeTimes->{{3.7165696277738323`*^9, 3.716569629504064*^9}, {
  3.7165697407336473`*^9, 3.716569752354289*^9}, {3.7165722241255627`*^9, 
  3.716572229769882*^9}, {3.7165734003174458`*^9, 3.716573403425645*^9}, {
  3.716653860693523*^9, 3.716653863413101*^9}, {3.716654940969427*^9, 
  3.7166549448558283`*^9}, {3.7166552536089573`*^9, 3.716655267051095*^9}, {
  3.7166564967801647`*^9, 3.7166565120667467`*^9}, {3.7166569620302763`*^9, 
  3.7166569738333797`*^9}, {3.716657645080573*^9, 3.716657697154606*^9}, {
  3.716659056644656*^9, 3.716659058409361*^9}},
 TextAlignment->Left,
 TextJustification->1.,ExpressionUUID->"4ac1749e-7cb1-405f-a027-540bb9275706"],

Cell[TextData[{
 "Let ",
 Cell[BoxData[
  FormBox[
   RowBox[{"f", ":", 
    RowBox[{
     RowBox[{"[", 
      RowBox[{"a", ",", "b"}], "]"}], "\[LongRightArrow]", 
     "\[DoubleStruckCapitalR]"}]}], TraditionalForm]],ExpressionUUID->
  "740e968d-40d9-42c2-abc0-6142e8f0abc8"],
 " be a  two-times differentiable function. "
}], "Text",
 CellChangeTimes->{{3.716569665755396*^9, 3.716569698456965*^9}, {
  3.7165697611456127`*^9, 3.716569864248426*^9}, {3.7165699012419567`*^9, 
  3.716569950864009*^9}, {3.7165722655009747`*^9, 3.716572300839004*^9}, {
  3.7165723334542923`*^9, 3.7165723729614363`*^9}, {3.7165734130933123`*^9, 
  3.716573603226646*^9}, {3.7166538759288273`*^9, 3.716653908969178*^9}, {
  3.7166549086126003`*^9, 3.7166549099481916`*^9}, {3.716656555223523*^9, 
  3.7166567516161823`*^9}, {3.716656978172309*^9, 3.716657077050304*^9}, {
  3.716657891437654*^9, 3.716657904764625*^9}, {3.716660456952936*^9, 
  3.716660457754985*^9}},
 TextAlignment->Left,
 TextJustification->1.,ExpressionUUID->"7d0c5b62-65a9-43c8-ac13-f6fbb46fbc83"],

Cell[TextData[{
 "(1st order condition) Find the singular points of ",
 Cell[BoxData[
  FormBox["f", TraditionalForm]],ExpressionUUID->
  "34acf6d0-e825-458e-b7d1-519680abd773"],
 ", solving the equation ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"f", "'"}], 
     RowBox[{"(", "t", ")"}]}], "=", "0"}], TraditionalForm]],ExpressionUUID->
  "197c9038-06e5-4a90-84bd-87bbb8d26a22"],
 "."
}], "Text",
 CellDingbat->"\[GrayCircle]",
 CellChangeTimes->{{3.716569665755396*^9, 3.716569698456965*^9}, {
  3.7165697611456127`*^9, 3.716569864248426*^9}, {3.7165699012419567`*^9, 
  3.716569950864009*^9}, {3.7165722655009747`*^9, 3.716572300839004*^9}, {
  3.7165723334542923`*^9, 3.7165723729614363`*^9}, {3.7165734130933123`*^9, 
  3.716573603226646*^9}, {3.7166538759288273`*^9, 3.716653908969178*^9}, {
  3.7166549086126003`*^9, 3.7166549099481916`*^9}, {3.716656555223523*^9, 
  3.7166567516161823`*^9}, {3.716656978172309*^9, 3.716657077050304*^9}, {
  3.716657891437654*^9, 3.7166579622311373`*^9}, {3.716659025382403*^9, 
  3.716659038080803*^9}},
 TextAlignment->Left,
 TextJustification->1.,ExpressionUUID->"83bebd10-e170-4734-9b49-03127e24e786"],

Cell[TextData[{
 "(2nd order condition) For each singular point ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["t", "0"], TraditionalForm]],ExpressionUUID->
  "d0d9e878-3e05-40e6-941d-297a8e326e10"],
 ", check the sign of ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"f", "''"}], 
    RowBox[{"(", 
     SubscriptBox["t", "0"], ")"}]}], TraditionalForm]],ExpressionUUID->
  "00ead671-6bd9-4ee1-b751-0d6aeed0cb49"],
 ". If it is positive, ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["t", "0"], TraditionalForm]],ExpressionUUID->
  "3227e353-658f-4669-a087-0685971b57ca"],
 " is a local minimum. If it is negative, ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["t", "0"], TraditionalForm]],ExpressionUUID->
  "b4cbc17d-36e2-4a71-b95e-9d16fc98ee76"],
 " is a local maximum."
}], "Text",
 CellDingbat->"\[GrayCircle]",
 CellChangeTimes->{{3.716569665755396*^9, 3.716569698456965*^9}, {
  3.7165697611456127`*^9, 3.716569864248426*^9}, {3.7165699012419567`*^9, 
  3.716569950864009*^9}, {3.7165722655009747`*^9, 3.716572300839004*^9}, {
  3.7165723334542923`*^9, 3.7165723729614363`*^9}, {3.7165734130933123`*^9, 
  3.716573603226646*^9}, {3.7166538759288273`*^9, 3.716653908969178*^9}, {
  3.7166549086126003`*^9, 3.7166549099481916`*^9}, {3.716656555223523*^9, 
  3.7166567516161823`*^9}, {3.716656978172309*^9, 3.716657077050304*^9}, {
  3.716657891437654*^9, 3.7166579622311373`*^9}, {3.716659025382403*^9, 
  3.716659038080803*^9}, {3.716659090429112*^9, 3.716659092309823*^9}, {
  3.716659432484107*^9, 3.71665953676468*^9}},
 TextAlignment->Left,
 TextJustification->1.,ExpressionUUID->"bd94065c-246f-4ec9-bc35-9e0931f8995f"],

Cell[CellGroupData[{

Cell["Example", "Subsubsection",
 CellChangeTimes->{{3.716657225818329*^9, 3.716657228071602*^9}, {
  3.71665957474657*^9, 3.7166595758019133`*^9}},
 TextAlignment->Left,
 TextJustification->1.,ExpressionUUID->"a548db70-89dc-4eb0-bc97-8f8f397900d9"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"f", "[", "t_", "]"}], ":=", 
  RowBox[{
   SuperscriptBox["t", "3"], "-", 
   RowBox[{"6", "t"}], "+", "2"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Print", "[", 
   RowBox[{"\"\<The differential of f[t] is \>\"", ",", " ", 
    RowBox[{
     RowBox[{"f", "'"}], "[", "t", "]"}]}], "]"}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"sol", "=", 
   RowBox[{"Solve", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"f", "'"}], "[", "t", "]"}], "\[Equal]", "0"}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Print", "[", 
   RowBox[{"\"\<The singular points of f[t] are \>\"", ",", " ", "sol"}], 
   "]"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{"Print", "[", 
  RowBox[{
  "\"\<The second differentials of f[t] in the singular points are \>\"", ",",
    " ", 
   RowBox[{"Map", "[", 
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{"f", "''"}], "[", "t", "]"}], "/.", "#"}], "&"}], ",", 
     "sol"}], "]"}]}], "]"}]}], "Input",
 CellChangeTimes->{{3.7166595789050713`*^9, 3.716659588419908*^9}, {
  3.716659637421089*^9, 3.716659935851511*^9}},
 TextAlignment->Left,
 TextJustification->1.,ExpressionUUID->"debad503-1ee6-4359-a74e-9043b8ae3260"],

Cell[TextData[{
 "Then ",
 Cell[BoxData[
  FormBox[
   RowBox[{"-", 
    SqrtBox["2"]}], TraditionalForm]],ExpressionUUID->
  "135b2622-ea29-4620-9a6b-74ce963ca9ed"],
 " is a local maximum and ",
 Cell[BoxData[
  FormBox[
   SqrtBox["2"], TraditionalForm]],ExpressionUUID->
  "daf822a0-7b8f-4bfe-932a-5cbf9d2a7d18"],
 " is a local minimum. Graphically,"
}], "Text",
 CellChangeTimes->{{3.716659947054134*^9, 3.7166600024285793`*^9}},
 TextAlignment->Left,
 TextJustification->1.,ExpressionUUID->"367116a6-c42f-496e-81eb-5771f9e338ca"],

Cell[BoxData[
 RowBox[{"Plot", "[", " ", 
