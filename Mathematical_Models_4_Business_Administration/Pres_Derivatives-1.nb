 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"(", 
      RowBox[{"sin", " ", 
       SubsuperscriptBox["t", "0", "3"]}], ")"}], "'"}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{"(", 
        RowBox[{"g", "\[SmallCircle]", "f"}], ")"}], "'"}], 
